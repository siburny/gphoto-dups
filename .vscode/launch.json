  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Renderer",
      "program": "${workspaceFolder}/renderer.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node",
    },
    {
      "name": "Run Get",
      "program": "${workspaceFolder}/get.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node",
    },
    {
      "name": "Run DB",
      "program": "${workspaceFolder}/db.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node",
    }
  ]
}